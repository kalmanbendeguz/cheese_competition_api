<!doctype html><html lang="<%= lang %>" class=notranslate translate=no><meta name=google content=notranslate><meta charset=UTF-8><meta name=viewport content="width=device-width,initial-scale=1"><meta data-name=confirm_remove_cheese_text content="<%= dict[lang].confirm_remove_cheese_text %>"><link rel=icon type=image/x-icon href=/static/images/MSE_1_32x32.png><title><%= dict[lang].application_name %></title><link href=https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css rel=stylesheet integrity=sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3 crossorigin=anonymous onerror='this.onerror=null,this.removeAttribute("integrity"),this.href="/static/css/cached/bootstrap.min.css"'><link rel=stylesheet href=/static/css/normalize.css><link rel=stylesheet href=/static/css/style_2.css><script src=/static/scripts/landing/landing.js defer></script> <%- include('barion_pixel_base') %>  <%- include('header_authenticated') %> <main class=container-fluid> <%- include('feedback') %> <div class="container col-xxl-4 col-xl-5 col-lg-6 col-sm-12 col-12"><div class=text-center><a class="w-20 btn btn-lg btn-primary mb-3" href=/new_cheese><%= dict[lang].new_cheese_button_text %></a></div> <% if(locals.paid_competition == false && ((locals?.unpaid_cheeses?.length ?? 0) === 0) && ((locals?.cheeses?.length ?? 0) !== 0)) { %> <div class="text-center mb-3"><span class="badge bg-success"><%= dict[lang].competition_is_free %></span></div> <% } else if(((locals?.unpaid_cheeses?.length ?? 0) === 0) && ((locals?.cheeses?.length ?? 0) !== 0)) { %> <div class="text-center mb-3"><span class="badge bg-success"><%= dict[lang].all_entry_fees_paid_text %></span></div> <% } else if((locals?.unpaid_cheeses?.length ?? 0) !== 0){ %> <div class="text-center mt-2"><span class="badge bg-warning"><%= dict[lang].not_all_entry_fees_are_paid_text %></span></div><div class="text-center mt-1 mb-1"><a class="w-20 btn btn-lg btn-info" href=/unpaid_entry_fee_payment><%= dict[lang].entry_fee_payment_button_text %></a></div><div class="text-center mb-3"><h5><span class="badge bg-info"> <%= dict[lang].entry_fee_to_pay_text %> <%= locals?.unpaid_cheeses?.length %> x <%= locals?.entry_fee_amount %> <%= locals?.entry_fee_currency %> = <%= locals?.entry_fee_total %> <%= locals?.entry_fee_currency %> </span></h5></div> <% } %> <% if((locals?.cheeses?.length ?? 0) === 0){ %> <div class=text-center><%= dict[lang].no_cheeses_entered_text %></div> <% } else if(Array.isArray(locals?.cheeses)) { %> <%- include('landing_pagination') %> <% for(let cheese of cheeses){ %> <div class="card mb-3"><div class=card-body><div class="row justify-content-space-between"><div class=col><h5><%= cheese?.product_name ?? "" %></h5></div><div class=col> <% if (locals.paid_competition == false) { %> <span class="badge bg-success"><%= dict[lang].competition_is_free %></span> <% } else if(cheese?.entry_certificate ?? false) { %> <span class="badge bg-success"><%= dict[lang].product_entried_as_association_member %></span> <% } else if(cheese?.entry_fee_paid ?? false) { %> <span class="badge bg-success"><%= dict[lang].product_entry_fee_paid %></span> <% } else if(cheese?.unpaid ?? false) { %> <span class="badge bg-danger"><%= dict[lang].product_unpaid %></span> <% } %> </div></div><div class="row mb-3 aligncenter"><div class="col-6 fw-bold"><%= dict[lang].cheese_id_prefix %></div><div class=col-6><%= cheese?.public_id ?? "" %></div></div><div class="row mb-3 aligncenter"><div class="col-6 fw-bold"><%= dict[lang].cheese_category_prefix %></div><div class=col-6><div><%= dict[lang][cheese?.product_category_list?.[0] ?? null] ?? "" %></div><div><%= dict[lang][cheese?.product_category_list?.at?.(-1) ?? null] ?? "" %></div></div></div><div class="row justify-content-space-between text-center aligncenter"><div class=col><form action=/view_cheese><input type=hidden name=public_id value="<%= cheese?.public_id ?? "" %>"> <input class="btn btn-sm btn-primary" type=submit value="<%= dict[lang].view_cheese_button_text %>"></form></div> <% if(cheese?.unpaid ?? false) { %> <div class=col><form action=/edit_cheese><input type=hidden name=public_id value="<%= cheese?.public_id ?? "" %>"> <input class="btn btn-sm btn-primary" type=submit value="<%= dict[lang].edit_cheese_button_text %>"></form></div><div class=col><form action=/remove_cheese class=remove_cheese_form method=POST><input type=hidden name=public_id value="<%= cheese?.public_id ?? "" %>"> <input class="btn btn-sm btn-primary" type=submit value="<%= dict[lang].remove_cheese_button_text %>"></form></div> <% } %> </div></div></div> <% } %> <%- include('landing_pagination') %> <% } %> </div></main> <%- include('footer') %> <script src=https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js integrity=sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p crossorigin=anonymous onerror='this.onerror=null,this.removeAttribute("integrity"),this.href="/static/scripts/cached/bootstrap.bundle.min.js"'></script>